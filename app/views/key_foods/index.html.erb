<div class="page-header">
  <h1>
    Key foods
    <a href="/key_foods/new" class="btn btn-lg btn-success">New Key food</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Actions</th>
      </tr>

      <% @key_foods.each do |key_food| %>
      <tr>
        <td><%= key_food.name %></td>
        <td>
          <a href="/key_foods/<%= key_food.id %>" class="btn btn-primary">Show</a>
          <a href="/key_foods/<%= key_food.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_key_food/<%= key_food.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
