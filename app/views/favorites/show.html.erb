<div class="page-header">
  <h3>Favorite #<%= @favorite.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">

      <dt>Recipe</dt>
      <dd><%= @favorite.recipe_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
